import pandas as pd

orders = pd.read_csv("../data/raw/orders.csv")
returns = pd.read_csv("../data/raw/returns.csv")

data = orders.merge(returns, on="order_id", how="left")
data["is_returned"] = data["return_id"].notnull().astype(int)

data.drop_duplicates(inplace=True)
data.to_csv("../data/processed/merged_data.csv", index=False)
